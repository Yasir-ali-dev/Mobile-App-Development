<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".WorkManagerActivity"
            android:exported="false"
            android:label="@string/title_activity_work_manager"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".ActivityHub"
            android:exported="true"
            android:label="@string/title_activity_hub"
            android:theme="@style/Theme.MyApplication">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NotificationService"
            android:exported="false"
            android:label="@string/title_activity_notification_service"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".ActivityNew"
            android:exported="true"
            android:label="@string/title_activity_new"
            android:theme="@style/Theme.MyApplication"></activity>
        <activity
            android:name=".ui.theme.Lab4_2"
            android:exported="true"
            android:label="@string/title_activity_lab42"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".Lab4_3"
            android:exported="true"
            android:label="@string/title_activity_lab43"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".Activity"
            android:exported="true"
            android:label="@string/title_activity_"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MyApplication" />
    </application>

</manifest>